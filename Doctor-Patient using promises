let n = Number(prompt('Enter the count : '));
let arr  = ['a','b','c','d','e','f','g','h','i','j'];
for(var i=0;i<n;i++)
{
    let index = Math.floor(Math.random()*(arr.length-1));
    let p = new Promise((resolve, reject) => 
    {
            
        let time = Math.floor(Math.random()*10000);
        setTimeout(() =>
        {
            resolve(arr[index]+' has taken '+time+' milliseconds');
        },time);
    });
        
    async function func()
    {
        let result = await p;
        console.log(result);
    }
    await func();
    arr.remove(index);
    
    
}

